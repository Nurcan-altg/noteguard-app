{
  "meta": {
    "version": "1.0.0",
    "type": "web-dashboard",
    "framework": "universal",
    "compatibility": ["Chrome 90+", "Firefox 88+", "Safari 14+", "Edge 90+"]
  },
  "theme": {
    "name": "Modern Dashboard Light",
    "mode": "light",
    "cssVariables": {
      "colors": {
        "--color-primary-50": "#f8fafc",
        "--color-primary-100": "#f1f5f9",
        "--color-primary-200": "#e2e8f0",
        "--color-primary-300": "#cbd5e1",
        "--color-primary-400": "#94a3b8",
        "--color-primary-500": "#64748b",
        "--color-primary-600": "#475569",
        "--color-primary-700": "#334155",
        "--color-primary-800": "#1e293b",
        "--color-primary-900": "#0f172a",
        "--gradient-primary": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
        "--gradient-secondary": "linear-gradient(135deg, #f093fb 0%, #f5576c 100%)",
        "--gradient-accent": "linear-gradient(135deg, #4facfe 0%, #00f2fe 100%)",
        "--gradient-success": "linear-gradient(135deg, #43e97b 0%, #38f9d7 100%)",
        "--gradient-warning": "linear-gradient(135deg, #fa709a 0%, #fee140 100%)",
        "--bg-primary": "#ffffff",
        "--bg-secondary": "#f8fafc",
        "--bg-tertiary": "#f1f5f9",
        "--text-primary": "#1e293b",
        "--text-secondary": "#64748b",
        "--text-tertiary": "#94a3b8",
        "--border-color": "#e2e8f0",
        "--success-color": "#10b981",
        "--warning-color": "#f59e0b",
        "--error-color": "#ef4444",
        "--info-color": "#3b82f6"
      },
      "spacing": {
        "--space-xs": "0.25rem",
        "--space-sm": "0.5rem",
        "--space-md": "1rem",
        "--space-lg": "1.5rem",
        "--space-xl": "2rem",
        "--space-2xl": "3rem",
        "--space-3xl": "4rem"
      },
      "typography": {
        "--font-family-primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
        "--font-family-mono": "'Fira Code', 'Cascadia Code', Consolas, monospace",
        "--font-size-xs": "0.75rem",
        "--font-size-sm": "0.875rem",
        "--font-size-base": "1rem",
        "--font-size-lg": "1.125rem",
        "--font-size-xl": "1.25rem",
        "--font-size-2xl": "1.5rem",
        "--font-size-3xl": "1.875rem",
        "--font-size-4xl": "2.25rem"
      },
      "layout": {
        "--sidebar-width-collapsed": "80px",
        "--sidebar-width-expanded": "280px",
        "--header-height": "72px",
        "--border-radius-sm": "0.375rem",
        "--border-radius-md": "0.5rem",
        "--border-radius-lg": "0.75rem",
        "--border-radius-xl": "1rem",
        "--border-radius-full": "9999px"
      }
    },
    "colors": {
      "primary": {
        "50": "#f8fafc",
        "100": "#f1f5f9",
        "200": "#e2e8f0",
        "300": "#cbd5e1",
        "400": "#94a3b8",
        "500": "#64748b",
        "600": "#475569",
        "700": "#334155",
        "800": "#1e293b",
        "900": "#0f172a"
      },
      "gradient": {
        "primary": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
        "secondary": "linear-gradient(135deg, #f093fb 0%, #f5576c 100%)",
        "accent": "linear-gradient(135deg, #4facfe 0%, #00f2fe 100%)",
        "success": "linear-gradient(135deg, #43e97b 0%, #38f9d7 100%)",
        "warning": "linear-gradient(135deg, #fa709a 0%, #fee140 100%)"
      },
      "background": {
        "primary": "#ffffff",
        "secondary": "#f8fafc",
        "tertiary": "#f1f5f9"
      },
      "text": {
        "primary": "#1e293b",
        "secondary": "#64748b",
        "tertiary": "#94a3b8"
      },
      "border": "#e2e8f0",
      "success": "#10b981",
      "warning": "#f59e0b",
      "error": "#ef4444",
      "info": "#3b82f6"
    },
    "webFonts": {
      "google": {
        "families": [
          {
            "name": "Inter",
            "weights": [300, 400, 500, 600, 700],
            "display": "swap"
          },
          {
            "name": "Fira Code",
            "weights": [400, 500],
            "display": "swap"
          }
        ],
        "url": "https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&family=Fira+Code:wght@400;500&display=swap"
      },
      "fallbacks": {
        "primary": ["-apple-system", "BlinkMacSystemFont", "'Segoe UI'", "Roboto", "sans-serif"],
        "mono": ["'Cascadia Code'", "Consolas", "monospace"]
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
        "mono": "'Fira Code', 'Cascadia Code', Consolas, monospace"
      },
      "fontSize": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem"
      },
      "fontWeight": {
        "normal": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      }
    },
    "spacing": {
      "xs": "0.25rem",
      "sm": "0.5rem",
      "md": "1rem",
      "lg": "1.5rem",
      "xl": "2rem",
      "2xl": "3rem",
      "3xl": "4rem"
    },
    "borderRadius": {
      "sm": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "xl": "1rem",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)"
    }
  },
  "layout": {
    "grid": {
      "system": "css-grid",
      "columns": 12,
      "gap": {
        "mobile": "1rem",
        "tablet": "1.5rem",
        "desktop": "2rem"
      },
      "container": {
        "fluid": true,
        "centered": true
      }
    },
    "flexbox": {
      "utilities": true,
      "alignment": "modern",
      "wrap": "responsive"
    },
    "breakpoints": {
      "mobile": "640px",
      "tablet": "768px",
      "desktop": "1024px",
      "large": "1280px",
      "xl": "1536px"
    },
    "container": {
      "padding": {
        "mobile": "1rem",
        "tablet": "1.5rem",
        "desktop": "2rem"
      },
      "maxWidth": {
        "mobile": "100%",
        "tablet": "768px",
        "desktop": "1024px",
        "large": "1280px",
        "xl": "1536px"
      }
    },
    "sidebar": {
      "width": {
        "collapsed": "80px",
        "expanded": "280px"
      },
      "background": "#ffffff",
      "border": "1px solid #e2e8f0"
    },
    "header": {
      "height": "72px",
      "background": "#ffffff",
      "border": "1px solid #e2e8f0"
    }
  },
  "components": {
    "webComponents": {
      "customElements": true,
      "shadowDOM": false,
      "htmlTemplates": true
    },
    "card": {
      "background": "#ffffff",
      "border": "1px solid #e2e8f0",
      "borderRadius": "0.75rem",
      "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "padding": {
        "mobile": "1rem",
        "desktop": "1.5rem"
      }
    },
    "button": {
      "primary": {
        "states": {
          "default": {
            "background": "var(--gradient-primary)",
            "color": "#ffffff",
            "border": "none",
            "borderRadius": "var(--border-radius-md)",
            "padding": "0.75rem 1.5rem",
            "fontSize": "var(--font-size-sm)",
            "fontWeight": 500,
            "cursor": "pointer",
            "transition": "all 300ms cubic-bezier(0.4, 0, 0.2, 1)"
          },
          "hover": {
            "transform": "translateY(-1px)",
            "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
            "filter": "brightness(1.05)"
          },
          "active": {
            "transform": "translateY(0)",
            "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
          },
          "disabled": {
            "opacity": 0.5,
            "cursor": "not-allowed",
            "transform": "none"
          },
          "loading": {
            "cursor": "wait",
            "opacity": 0.8
          }
        }
      },
      "secondary": {
        "states": {
          "default": {
            "background": "#ffffff",
            "color": "var(--text-secondary)",
            "border": "1px solid var(--border-color)",
            "borderRadius": "var(--border-radius-md)",
            "padding": "0.75rem 1.5rem",
            "fontSize": "var(--font-size-sm)",
            "fontWeight": 500,
            "cursor": "pointer",
            "transition": "all 300ms cubic-bezier(0.4, 0, 0.2, 1)"
          },
          "hover": {
            "background": "var(--bg-secondary)",
            "borderColor": "var(--color-primary-300)",
            "transform": "translateY(-1px)"
          },
          "active": {
            "background": "var(--bg-tertiary)",
            "transform": "translateY(0)"
          }
        }
      }
    },
    "input": {
      "background": "#ffffff",
      "border": "1px solid #e2e8f0",
      "borderRadius": "0.5rem",
      "padding": "0.75rem 1rem",
      "fontSize": "0.875rem",
      "color": "#1e293b",
      "placeholder": "#94a3b8",
      "focus": {
        "borderColor": "#667eea",
        "shadow": "0 0 0 3px rgba(102, 126, 234, 0.1)"
      }
    },
    "navigation": {
      "item": {
        "padding": "0.75rem 1rem",
        "borderRadius": "0.5rem",
        "margin": "0.25rem 0",
        "fontSize": "0.875rem",
        "fontWeight": 500,
        "color": "#64748b",
        "hover": {
          "background": "#f8fafc",
          "color": "#1e293b"
        },
        "active": {
          "background": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
          "color": "#ffffff"
        }
      }
    },
    "metrics": {
      "card": {
        "background": "#ffffff",
        "border": "1px solid #e2e8f0",
        "borderRadius": "0.75rem",
        "padding": "1.5rem",
        "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
        "title": {
          "fontSize": "0.875rem",
          "fontWeight": 500,
          "color": "#64748b",
          "marginBottom": "0.5rem"
        },
        "value": {
          "fontSize": "2.25rem",
          "fontWeight": 700,
          "color": "#1e293b",
          "marginBottom": "0.25rem"
        },
        "change": {
          "fontSize": "0.75rem",
          "fontWeight": 500,
          "positive": "#10b981",
          "negative": "#ef4444"
        }
      }
    },
    "chart": {
      "background": "#ffffff",
      "border": "1px solid #e2e8f0",
      "borderRadius": "0.75rem",
      "padding": "1.5rem",
      "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
      "colors": [
        "#667eea",
        "#764ba2",
        "#4facfe",
        "#00f2fe",
        "#43e97b",
        "#38f9d7",
        "#fa709a",
        "#fee140"
      ]
    },
    "table": {
      "background": "#ffffff",
      "border": "1px solid #e2e8f0",
      "borderRadius": "0.75rem",
      "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
      "header": {
        "background": "#f8fafc",
        "color": "#64748b",
        "fontSize": "0.75rem",
        "fontWeight": 600,
        "textTransform": "uppercase",
        "letterSpacing": "0.05em",
        "padding": "0.75rem 1rem"
      },
      "row": {
        "padding": "1rem",
        "borderBottom": "1px solid #f1f5f9",
        "hover": {
          "background": "#f8fafc"
        }
      }
    },
    "status": {
      "badge": {
        "padding": "0.25rem 0.75rem",
        "borderRadius": "9999px",
        "fontSize": "0.75rem",
        "fontWeight": 500,
        "success": {
          "background": "#dcfce7",
          "color": "#166534"
        },
        "warning": {
          "background": "#fef3c7",
          "color": "#92400e"
        },
        "error": {
          "background": "#fecaca",
          "color": "#991b1b"
        },
        "info": {
          "background": "#dbeafe",
          "color": "#1e40af"
        }
      }
    },
    "avatar": {
      "sizes": {
        "sm": "2rem",
        "md": "2.5rem",
        "lg": "3rem",
        "xl": "4rem"
      },
      "borderRadius": "9999px",
      "border": "2px solid #ffffff",
      "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
    }
  },
  "icons": {
    "library": "lucide-react",
    "size": {
      "sm": "1rem",
      "md": "1.25rem",
      "lg": "1.5rem",
      "xl": "2rem"
    },
    "strokeWidth": 2,
    "color": {
      "primary": "#64748b",
      "secondary": "#94a3b8",
      "active": "#667eea"
    }
  },
  "animations": {
    "duration": {
      "fast": "150ms",
      "normal": "300ms",
      "slow": "500ms"
    },
    "easing": {
      "ease": "cubic-bezier(0.4, 0, 0.2, 1)",
      "easeIn": "cubic-bezier(0.4, 0, 1, 1)",
      "easeOut": "cubic-bezier(0, 0, 0.2, 1)",
      "easeInOut": "cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "transitions": {
      "default": "all 300ms cubic-bezier(0.4, 0, 0.2, 1)",
      "colors": "color 150ms cubic-bezier(0.4, 0, 0.2, 1), background-color 150ms cubic-bezier(0.4, 0, 0.2, 1)",
      "transform": "transform 300ms cubic-bezier(0.4, 0, 0.2, 1)"
    }
  },
  "responsive": {
    "mobile": {
      "sidebar": {
        "display": "none"
      },
      "header": {
        "padding": "1rem"
      },
      "content": {
        "padding": "1rem"
      },
      "cards": {
        "grid": "1fr",
        "gap": "1rem"
      }
    },
    "tablet": {
      "sidebar": {
        "width": "80px",
        "collapsed": true
      },
      "header": {
        "padding": "1rem 1.5rem"
      },
      "content": {
        "padding": "1.5rem"
      },
      "cards": {
        "grid": "repeat(2, 1fr)",
        "gap": "1.5rem"
      }
    },
    "desktop": {
      "sidebar": {
        "width": "280px",
        "collapsed": false
      },
      "header": {
        "padding": "1rem 2rem"
      },
      "content": {
        "padding": "2rem"
      },
      "cards": {
        "grid": "repeat(4, 1fr)",
        "gap": "2rem"
      }
    }
  },
  "accessibility": {
    "focusRing": {
      "color": "#667eea",
      "width": "2px",
      "offset": "2px",
      "style": "solid"
    },
    "contrast": {
      "minimum": "4.5:1",
      "enhanced": "7:1"
    }
  },
  "webOptimization": {
    "performance": {
      "lazyLoading": true,
      "imageOptimization": {
        "formats": ["webp", "avif", "jpg", "png"],
        "sizes": ["320w", "640w", "768w", "1024w", "1280w", "1536w"],
        "quality": 85
      },
      "codesplitting": true,
      "bundleOptimization": true
    },
    "seo": {
      "metaTags": {
        "viewport": "width=device-width, initial-scale=1.0",
        "themeColor": "#667eea",
        "description": "Modern responsive dashboard interface"
      },
      "openGraph": {
        "type": "website",
        "siteName": "Dashboard"
      }
    },
    "pwa": {
      "manifest": {
        "name": "Dashboard App",
        "shortName": "Dashboard",
        "display": "standalone",
        "orientation": "portrait",
        "themeColor": "#667eea",
        "backgroundColor": "#ffffff"
      },
      "serviceWorker": true,
      "offline": true
    }
  },
  "css": {
    "framework": "utility-first",
    "customProperties": true,
    "prefixing": "autoprefixer",
    "minification": true,
    "criticalCSS": true,
    "resetCSS": {
      "type": "modern-normalize",
      "customReset": true
    },
    "utilities": {
      "spacing": "geometric-scale",
      "colors": "semantic-naming",
      "typography": "fluid-scaling",
      "components": "bem-methodology"
    }
  },
  "javascript": {
    "framework": "framework-agnostic",
    "modules": "es6",
    "bundler": "webpack/vite",
    "features": {
      "darkModeToggle": true,
      "responsiveSidebar": true,
      "lazyComponents": true,
      "virtualScrolling": true,
      "infiniteScroll": true,
      "searchFiltering": true,
      "dataVisualization": true
    }
  }
}